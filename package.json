{
  "name": "ui-auto-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf test-results report reports",
    "debug": "cross-env ENV=prod PWDEBUG=1 DEBUG=pw:api cucumber-js --config=test/config/cucumber.js",
    "test": "cross-env ENV=prod cucumber-js --config=test/config/cucumber.js",
    "test:auth": "cross-env ENV=prod cucumber-js --config=test/config/cucumber.js --profile auth || true",
    "test:h5": "cross-env ENV=prod cucumber-js --config=test/config/cucumber.js --profile h5 || true",
    "test:online": "cross-env ENV=prod cucumber-js --config=test/config/cucumber.js --profile online || true",
    "test:h5:prd": "cross-env ENV=prod cucumber-js --config=test/config/cucumber.js --profile h5_prd || true",
    "test:online:prd": "cross-env ENV=prod cucumber-js --config=test/config/cucumber.js --profile online_prd || true",
    "test:failed": "cucumber-js --config=test/config/cucumber.js --profile rerun @rerun.txt",
    "test:local": "cucumber-js --config=test/config/cucumber.js --profile local @local.txt",
    "test:ctest_noauth": "cucumber-js --config=test/config/cucumber.js --profile ctest @ctest.txt",
    "test:ctest:h5_noauth": "cucumber-js --config=test/config/cucumber.js --profile ctest_h5 @ctest.txt",
    "test:ctest:pc_noauth": "cucumber-js --config=test/config/cucumber.js --profile ctest_pc @ctest.txt",
    "test:ctest": "npm-run-all -s test:auth test:ctest_noauth",
    "test:ctest:h5": "npm-run-all -s test:auth test:ctest:h5_noauth",
    "test:ctest:pc": "npm-run-all -s test:auth test:ctest:pc_noauth",
    "debug:local": "PWDEBUG=1 DEBUG=pw:api npm run test:local",
    "test:demo": "cross-env ENV=prod cucumber-js --config=test/config/cucumber.js --profile demo || true",
    "report": "rm -rf reports && ts-node lib/scripts/report/index.ts",
    "lint": "eslint --quiet test lib",
    "lint:fix": "eslint --fix test lib",
    "lint-staged": "lint-staged",
    "husky": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git@git.dev.sh.ctripcorp.com:ychengzhang/ui-auto-test.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "9.6.0",
    "@playwright/test": "1.48.1",
    "assert": "^2.1.0",
    "base64url": "3.0.1",
    "crypto": "^1.0.1",
    "fs-extra": "^11.2.0",
    "jimp": "^0.22.12",
    "jsqr": "^1.4.0",
    "multiple-cucumber-html-reporter": "3.5.0",
    "request": "^2.88.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cucumber": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^12.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
